{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "laravel-docs",
  "title": "Laravel Docs",
  "description": "An ultra-fast Laravel docs search extension for Raycast.",
  "icon": "laravel-logo.png",
  "author": "simonellensohn",
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Laravel Docs",
      "description": "Search for anything in the Laravel documentation.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "version",
      "type": "dropdown",
      "required": true,
      "title": "Version",
      "description": "Specify the Laravel version to search the docs for.",
      "default": "9.x",
      "data": [
        {
          "title": "master",
          "value": "master"
        },
        {
          "title": "9.x",
          "value": "9.x"
        },
        {
          "title": "8.x",
          "value": "8.x"
        },
        {
          "title": "7.x",
          "value": "7.x"
        },
        {
          "title": "6.x",
          "value": "6.x"
        },
        {
          "title": "5.8",
          "value": "5.8"
        },
        {
          "title": "5.7",
          "value": "5.7"
        },
        {
          "title": "5.6",
          "value": "5.6"
        },
        {
          "title": "5.5",
          "value": "5.5"
        },
        {
          "title": "5.4",
          "value": "5.4"
        },
        {
          "title": "5.3",
          "value": "5.3"
        },
        {
          "title": "5.2",
          "value": "5.2"
        },
        {
          "title": "5.1",
          "value": "5.1"
        },
        {
          "title": "5.0",
          "value": "5.0"
        },
        {
          "title": "4.2",
          "value": "4.2"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.30.3",
    "@types/lodash": "^4.14.180",
    "algoliasearch": "^4.13.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
